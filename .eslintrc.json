{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/no-cycle":0,
    "comma-dangle":0,
    "react/forbid-prop-types":0,
    "react/jsx-props-no-spreading":0,
    "no-shadow":0,
    "no-param-reassign":0,
    "react/destructuring-assignment":0,
    "consistent-return":0
  }
}
